{
  "compilerOptions": {
    "allowJs": true,
    "outDir": "./dist/js",
    "noImplicitAny": true,
    "sourceMap": true,
    "module": "esnext",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "target": "es5",
    "lib": ["esnext", "dom"],
    "jsx": "react",
    "baseUrl": ".",
    "downlevelIteration": true,
    "paths": {
      "*": ["js/*"]
    }
  },
  "include": ["./js/**/*"]
}
