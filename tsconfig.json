{
  "compilerOptions": {
    "allowJs": true,
    "baseUrl": ".",
    "downlevelIteration": true,
    "esModuleInterop": true,
    "jsx": "react",
    "lib": ["esnext", "dom"],
    "module": "esnext",
    "moduleResolution": "node",
    "noImplicitAny": true,
    "outDir": "./dist/js",
    "paths": {
      "*": ["js/*"]
    },
    "sourceMap": true,
    "strictNullChecks": true,
    "useUnknownInCatchVariables": true,
    "target": "es5"
  },
  "include": ["./js/**/*", "./js/setupTests.ts"]
}
